{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "plugin:@typescript-eslint/recommended",
    "plugin:@next/next/recommended",
    "prettier"
  ],
  "ignorePatterns": [
    ".eslintrc.js",
    "next-env.d.ts",
    "next.config.js",
    "node_modules/"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "project": [
      "tsconfig.json"
    ],
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "root": true,
  "rules": {
    "arrow-body-style": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0,
    "no-html-link-for-pages": 0,
    "no-plusplus": 0,
    "react/destructuring-assignment": 0,
    "react/react-in-jsx-scope": 0
  }
}